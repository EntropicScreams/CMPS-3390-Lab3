<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="StudentGui">
  <grid id="27dc6" binding="panel1" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <scrollpane id="db78e">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="line" title="Student List" title-justification="2">
          <font/>
          <color color="-16777216"/>
        </border>
        <children>
          <component id="abb43" class="javax.swing.JList" binding="studentList">
            <constraints/>
            <properties/>
          </component>
        </children>
      </scrollpane>
      <grid id="1df95" binding="studentInput" layout-manager="GridBagLayout">
        <constraints border-constraint="East"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="de9cc" class="javax.swing.JLabel" binding="labelFirstName">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag top="5" left="10" bottom="5" right="10" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <text value="First Name"/>
            </properties>
          </component>
          <component id="98234" class="javax.swing.JTextField" binding="inputFirstName">
            <constraints>
              <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
              <gridbag weightx="0.0" weighty="0.0" ipadx="5" ipady="3"/>
            </constraints>
            <properties>
              <columns value="10"/>
            </properties>
          </component>
          <component id="79ecd" class="javax.swing.JLabel" binding="labelLastName">
            <constraints>
              <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag top="5" left="10" bottom="5" right="10" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <text value="Last Name"/>
            </properties>
          </component>
          <component id="a2479" class="javax.swing.JTextField" binding="inputLastName">
            <constraints>
              <grid row="1" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
              <gridbag weightx="0.0" weighty="0.0" ipadx="5" ipady="3"/>
            </constraints>
            <properties/>
          </component>
          <component id="ef9fe" class="javax.swing.JLabel" binding="labelStudentID">
            <constraints>
              <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag top="5" left="10" bottom="5" right="10" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <text value="Student ID"/>
            </properties>
          </component>
          <component id="492fd" class="javax.swing.JTextField" binding="inputStudentID">
            <constraints>
              <grid row="2" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
              <gridbag weightx="0.0" weighty="0.0" ipadx="5" ipady="3"/>
            </constraints>
            <properties/>
          </component>
          <vspacer id="79337">
            <constraints>
              <grid row="3" column="2" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
              <gridbag weightx="0.0" weighty="2.0"/>
            </constraints>
          </vspacer>
          <hspacer id="61f2a">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
              <gridbag weightx="0.0" weighty="0.0"/>
            </constraints>
          </hspacer>
          <hspacer id="f3bc9">
            <constraints>
              <grid row="0" column="3" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
              <gridbag weightx="0.0" weighty="0.0"/>
            </constraints>
          </hspacer>
        </children>
      </grid>
      <grid id="9ff05" binding="studentAction" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="7fbfc" class="javax.swing.JButton" binding="buttonAddStudent">
            <constraints/>
            <properties>
              <text value="Assimilate Student"/>
            </properties>
          </component>
          <component id="f56d7" class="javax.swing.JButton" binding="buttonDelStudent">
            <constraints/>
            <properties>
              <text value="Remove Student"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
